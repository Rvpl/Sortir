<div class="container-formMofif">
    {{ form_start(form) }}
    <h1>Modification du profil</h1>
    <div class="formMofifInf">
        <div class="formMofif">{{ form_row(form.pseudo) }}</div>
        <div class="formMofif">{{ form_row(form.nom) }}</div>
    </div>
    <div class="formMofifInf">
        <div class="formMofif">{{ form_row(form.prenom) }}</div>
        <div class="formMofif">{{ form_row(form.telephone) }}</div>
    </div>
    <div class="formMofifInf">
        <div class="formMofif">{{ form_row(form.email) }}</div>
        <div class="formMofif">{{ form_row(form.plainPassword) }}</div>
    </div>
    <div class="formMofifInf">
        <div class="formMofif">{{ form_row(form.photoProfil) }}</div>
        <div class="formMofif">{{ form_row(form.campus) }}</div>
    </div>
    <button type="submit" class="btn-formMofif">Enregistrer</button>
    {{ form_end(form) }}
</div>
